<!-- create family form -->
<div class="container">
  <%= @family.name %>
  <% @tasks.each do |task|%>
    <div class="card p-5">
      <%= task.title %>
      <%= task.description %>
      <%= task.due_date %>
    <% end %>
  </div>
  <%= link_to "Create Tasks",  family_tasks_path(@family), class: "btn btn-success" %>
</div>
